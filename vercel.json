{
  "version": 2,
  "builds": [
    {
      "src": "server/app.js",
      "use": "@vercel/node"
    }
  ],
  "functions": {
    "server/app.js": {
      "includeFiles": "{assets,components,views,scripts}/**"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server/app.js"
    }
  ]
}
